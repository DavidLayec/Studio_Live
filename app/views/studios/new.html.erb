<%= simple_form_for(@studio) do |f| %>
<%= f.error_notification %>

  <div class="form-inputs">
  <%= f.input :title %>
  <%= f.input :address, id: "studio_address"%>
  <%= f.input :description %>
  <%= f.input :price %>
  <%=  f.input :photo, as: :file  %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
